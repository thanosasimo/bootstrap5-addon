{
  "name": "bootstrap5-addon",
  "version": "7.0.6",
  "description": "> Extra css classes and helpers for bootstrap 5 grid system.",
  "main": "index.js",
  "scripts": {
    "build": "node-sass --output-style compressed sass/bootstrap5-addon.scss -o dist",
